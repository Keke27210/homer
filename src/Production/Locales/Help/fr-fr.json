{
  "about": {
    "description": "Informations sur le bot",
    "usage": "about"
  },
  "feedback": {
    "description": "Dites-moi ce que vous pensez du bot, signaler des bugs, etc...",
    "usage": "feedback <message>",
    "examples": ["feedback Votre bot est génial !", "feedback Il y a un bug lorsqu'on exécute la commande support :x"]
  },
  "help": {
    "description": "Obtenez de l'aide sur l'utilisation du bot",
    "usage": "help"
  },
  "invite": {
    "description": "Invitez le bot sur votre serveur",
    "usage": "invite"
  },
  "ping": {
    "description": "Pong!",
    "usage": "ping"
  },
  "support": {
    "description": "Rejoignez le serveur d'assistance pour obtenir de l'aide avancée",
    "usage": "support"
  },
  "translations": {
    "description": "Obtenez la liste des langues disponibles sur Homer",
    "usage": "translations"
  },

  "avatar": {
    "description": "Obtenez l'avatar de quelqu'un",
    "usage": "avatar [Utilisateur]",
    "examples": ["avatar", "avatar b1nzy#1337"]
  },
  "checkdbans": {
    "description": "Vérifie si un ID utilisateur est listé sur la liste Discord Bans (non-officiel)",
    "usage": "checkdbans ([Utilisateur]|[UserID)",
    "examples": ["checkdbans", "checkdbans b1nzy#1337", "checkdbans 657385946273049384"]
  },
  "google": {
    "description": "Cherche du contenu sur Google",
    "usage": "google <recherche>",
    "examples": ["google Quel est le meilleur moyen pour importer un module Python ?"]
  },
  "lookup": {
    "description": "Cherche des serveurs, des utilisateurs et des invitations à travers Discord",
    "usage": "lookup <(guildID|userID|invitation)>",
    "examples": ["lookup 205427654042583040", "lookup 382951433378594817", "lookup https://discord.gg/Z9mKZV6"]
  },
  "server": {
    "description": "Obtenez des informations sur le serveur",
    "usage": "server"
  },
  "translate": {
    "description": "Traduit le texte fourni dans la langue donnée",
    "usage": "translate <langue cible (ISO 639-1)> <texte>",
    "examples": ["translate fr Hello, how are you?"]
  },
  "user": {
    "description": "Obtenez des informations sur quelqu'un",
    "usage": "user [Utilisateur]",
    "examples": ["user", "user b1nzy#1337"]
  },
  "weather": {
    "description": "Obtenez la météo pour la localité donnée",
    "usage": "weather <localité>",
    "examples": ["weather Paris, France"]
  },
  "youtube": {
    "description": "Cherche du contenu sur YouTube",
    "usage": "youtube <recherche>",
    "examples": ["youtube Vidéos incroyables de Logan Paul"]
  },

  "afk": {
    "description": "Vous marque absent jusqu'à votre retour (AFK)",
    "usage": "afk [reaison]",
    "examples": ["afk Je fais mes devoirs, à tout à l'heure !"]
  },
  "choose": {
    "description": "Choisis aléatoirement entre deux ou plusieurs choix",
    "usage": "choose <choix A> <choix B> [choix C] etc.",
    "examples": ["choose iDroid Artuto iBot Endless"]
  },
  "profile": {
    "description": "Affichez le profile de quelqu'un",
    "usage": "profile ([Utilisateur]|set <nomChamp> <contenuChamp>|clear <nomChamp>)",
    "examples": ["profile iDroid#4441", "profile", "profile set twitter iDroid27210", "profile clear twitter"]
  },
  "remind": {
    "description": "Vous rappelle d'une tâche à faire",
    "usage": "remind (<durée> <description>|list|remove <index>)",
    "examples": ["remind 1h Je prends ma douche", "remind list", "remind remove 2"]
  },
  "say": {
    "description": "Répète ce que vous dites au bot",
    "usage": "say <texte>",
    "examples": ["say Je suis le meilleur bot jamais conçu... pstt c'est pas vrai mais chut !"]
  },
  "tag": {
    "description": "Jouez avec des commandes avancées !",
    "usage": "tag (sous-commande <args>)|<nomTag>",
    "examples": ["tag create <nomTag> <contenuTag>", "tag delete <nomTag>", "tag edit <nomTag> <nouveauContenu>", "tag owner <nomTag>", "tag search <rechercheTag>", "tag random", "tag raw <nomTag>", "tag <nomTag>"]
  },
  "timezone": {
    "description": "Quelle heure est-il là ?",
    "usage": "timezone (set <fuseau horaire>)|([Membre])",
    "examples": ["timezone set Europe/Paris", "timezone iDroid#4441"]
  },

  "format": {
    "description": "Configure le format de la date et de l'heure pour le serveur",
    "usage": "format <type> <format>",
    "examples": ["format date DD/MM/YYYY", "format time HH:mm:ss"]
  },
  "ignore": {
    "description": "Faire ignorer ou écouter un salon au bot",
    "usage": "ignore [Salon]",
    "examples": ["ignore #trash", "ignore partOfChannelName"]
  },
  "leave": {
    "description": "Configure le flux de départs",
    "usage": "leave <message>",
    "examples": ["leave Oh non, {user.tag} est parti !"]
  },
  "locale": {
    "description": "Configure la langue du serveur",
    "usage": "locale <code>",
    "examples": ["locale en-gb", "locale de-de", "locale fr-fr"]
  },
  "prefix": {
    "description": "Gérez vos préfixes personnalisés",
    "usage": "prefix [préfixe]",
    "examples": ["prefix bot!", "prefix homer::"]
  },
  "settimezone": {
    "description": "Configure le fuseau horaire du sevreur",
    "usage": "timezone <fuseau horaire>",
    "examples": ["timezone Europe/Paris", "timezone America/Mexico_City"]
  },
  "welcome": {
    "description": "Configure le flux d'arrivées",
    "usage": "welcome <message>",
    "examples": ["welcome Yeah, {user.tag} rentre dans la place !"]
  },

  "call": {
    "description": "Appelle un numéro de téléphone",
    "usage": "call <numéro>",
    "examples": ["call 111-222"]
  },
  "hangup": {
    "description": "Met fin à une communication",
    "usage": "hangup"
  },
  "number": {
    "description": "Affiche votre numéro de téléphone",
    "usage": "number [reset]",
    "examples": ["number", "number reset"]
  },
  "pickup": {
    "description": "Décrocher un appel en attente",
    "usage": "pickup",
    "examples": []
  },
  "telephone": {
    "description": "Configure le salon téléphone",
    "usage": "telephone",
    "examples": []
  },

  "moderation": {
    "description": "Aucune description disponible",
    "usage": "moderation",
    "examples": []
  }
}
